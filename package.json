{
  "name": "ops-models",
  "version": "1.0.1",
  "description": "Common models to be shared between web app, mobile app, electron app.",
  "scripts": {
    "build": "npx tsc",
    "lint": "eslint --ext .tsx --ext .ts src/",
    "lint:fix": "eslint --ext .tsx --ext .ts src/ --fix",
    "test": "jest --verbose",
    "test:update": "jest --verbose --coverage --updateSnapshot",
    "test:watch": "jest --verbose --watch",
    "test:coverage": "jest --verbose --coverage && open ./coverage/lcov-report/index.html",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/dewynguyen/ops-models.git"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "keywords": [],
  "author": "Catavin Software LLC",
  "private": true,
  "homepage": "https://bitbucket.org/dewynguyen/ops-models#readme",
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "eslint": "^8.2.0",
    "jest": "^27.3.1",
    "ts-jest": "^27.0.7",
    "tslib": "^2.3.1",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "moment": "^2.29.1"
  }
